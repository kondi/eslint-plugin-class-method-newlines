    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "enhanced-resolve-jest": "^1.1.0",
    "eslint": "^8.9.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
